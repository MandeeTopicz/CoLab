{
  "emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "board-backend",
          "region": "us-central1"
        }
      },
      {
        "source": "/ws",
        "run": {
          "serviceId": "board-backend",
          "region": "us-central1"
        }
      },
      {
        "source": "/ws/**",
        "run": {
          "serviceId": "board-backend",
          "region": "us-central1"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
